{{#if mesg1}}
<form action = "/stud_update" class="form1" role="search" >
{{#data}}
<input type="text" name="sid" placeholder="ID" value="{{s_id}}" required>
<input type="text" name="sname" placeholder="Name" value="{{sname}}" required>
<input type="text" name="cgpa" placeholder="CGPA" value="{{cgpa}}" required>
<input type="text" name="branch" placeholder="Branch" value="{{branch}}" required>
<input type="text" name="division" placeholder="Division" value="{{division}}" required>
<input type="text" name="internship" placeholder="Internship" value="{{internship}}" required>
<input type="text" name = "projectid" placeholder="Project ID" value="{{project_id}}" required>
<input type="text" name="domain" placeholder="Domain" value="{{domain}}" required>
<input type="text" name="clubname" placeholder="Clubname" value="{{clubname}}" required>
<input type="text" name="researchpaper" placeholder="Research Papers" value="{{researchpaper}}" required>
<input type="text" name="registrationid" placeholder="Registration Number" value="{{registration_no}}" required>
<button class="button">Submit</button>
{{/data}}
{{#mesg2}}<input class="button" type="reset" value="Reset">{{/mesg2}}
{{#if umesg}}
<h3>Data updated!</h3>
{{/if}}
</form>
{{/if}}
